<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="nor">

    <!-- Find user by email -->
    <select id="findByEmail" parameterType="string" resultType="com.ict.project.vo.personnel.UserVO">
        SELECT emp_email, emp_name, emp_password, home, detail_home, phone_number
        FROM users
        WHERE emp_email = #{empEmail}
    </select>

</mapper>