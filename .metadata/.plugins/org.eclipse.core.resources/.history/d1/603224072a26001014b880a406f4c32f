<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="nor">
	<select id="loginWithRole" parameterType="String" resultType="map" >
	 SELECT * 
  	FROM Users u
  	JOIN Employee e ON u.user_idx = e.user_idx
  	WHERE u.user_idx = #{user_idx}
	</select>
	
	
	
</mapper>















